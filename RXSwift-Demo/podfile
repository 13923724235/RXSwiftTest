# platform :ios, '9.0'

inhibit_all_warnings!
source 'https://github.com/CocoaPods/Specs.git'

target 'RXSwiftDemo' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'SnapKit'
  pod 'SwiftyJSON'
  pod 'Moya/RxSwift'
  pod 'RxDataSources'
  pod 'Alamofire'
  pod 'Kingfisher'

 end
